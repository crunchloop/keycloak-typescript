    "@typescript-eslint/parser": "^6.0.0",
    "jest": "^29.5.0",
    "jest-mock-extended": "^3.0.4",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^5.0.0",
    "prettier": "^3.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
